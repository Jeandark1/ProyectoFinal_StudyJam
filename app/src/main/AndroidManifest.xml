<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdandroidbolivia.babymater11">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/emoticono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Splashscreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimalesActivity" />
        <activity android:name=".VocalesActivity" />
        <activity android:name=".ColoresActivity" />
        <activity android:name=".NumerosActivity" />
        <activity android:name=".SentidosActivity"></activity>
    </application>

</manifest>